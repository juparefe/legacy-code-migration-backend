{
  "name": "legacy-code-migration-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package:lambda": "npm ci && npm run build && powershell -Command \"Remove-Item -Force .\\lambda.zip -ErrorAction SilentlyContinue; Compress-Archive -Path dist,node_modules,package.json -DestinationPath lambda.zip\"",
    "start": "node dist/server.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "debug": "^4.4.3",
    "express": "^4.19.2",
    "serverless-http": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/cors": "^2.8.19",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.33",
    "@types/supertest": "^6.0.3",
    "eslint": "~9.39.3",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.56.0"
  }
}
